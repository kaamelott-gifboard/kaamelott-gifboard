{
    "name": "kaamelott-gifboard/kaamelott-gifboard",
    "type": "project",
    "description": "Le recueil où l'on peut trouver les meilleures répliques de Kaamelott en GIFs",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Tommasi",
            "role": "Developer",
            "email": "tommasi.v@gmail.com"
        },
        {
            "name": "Pablo Godinez",
            "role": "Developer",
            "email": "pablo.godinez@zayon.eu"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "abraham/twitteroauth": "^3.2",
        "composer/package-versions-deprecated": "1.11.99.1",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpstan/phpstan": "^1.0",
        "symfony/asset": "^5.1",
        "symfony/browser-kit": "^5.1",
        "symfony/console": "^5.1",
        "symfony/dotenv": "^5.1",
        "symfony/expression-language": "^5.1",
        "symfony/finder": "^5.1",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.1",
        "symfony/http-client": "^5.1",
        "symfony/monolog-bundle": "^3.6",
        "symfony/property-access": "^5.1",
        "symfony/property-info": "^5.1",
        "symfony/security-bundle": "^5.1",
        "symfony/serializer": "^5.1",
        "symfony/string": "^5.1",
        "symfony/twig-bundle": "^5.1",
        "symfony/validator": "^5.1",
        "symfony/yaml": "^5.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/stopwatch": "^5.1",
        "symfony/web-profiler-bundle": "^5.1"
    },
    "config": {
        "platform": {
            "php": "8.0.2"
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KaamelottGifboard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KaamelottGifboard\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.1"
        }
    }
}
